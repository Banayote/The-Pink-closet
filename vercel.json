{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/store.html",
      "dest": "/public/store.html"
    },
    {
      "src": "/profile.html",
      "dest": "/public/profile.html"
    },
    {
      "src": "/messages.html",
      "dest": "/public/messages.html"
    },
    {
      "src": "/add-product.html",
      "dest": "/public/add-product.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/public/index.html"
    }
  ]
}
